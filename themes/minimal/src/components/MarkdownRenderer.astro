---
import { MarkdocRenderer } from "@local/markdoc-renderer";
import type { Content, Components } from "@local/markdoc-renderer";
import YouTubeEmbed from "./YouTubeEmbed.astro";

type Props = {
  content: Content;
};

const { content } = Astro.props;

const components = {
  YouTubeEmbed: {
    Component: YouTubeEmbed,
    props: {},
  },
};
---

<MarkdocRenderer content={content} components={components} />
